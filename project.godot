[internationalization]

locale/translations=PackedStringArray("res://translation/translation_utf8.en.translation", "res://translation/translation_utf8.hu.translation")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
